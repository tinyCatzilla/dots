{
    "preset": "laravel",
    "rules": {
        "blank_line_before_statement": false,
        "concat_space": {
            "spacing": "none"
        },
        "no_unused_imports": true,
        "declare_strict_types": true,
        "method_argument_space": true,
        "single_trait_insert_per_statement": true,
        "types_spaces": {
            "space": "single"
        }
    }
}